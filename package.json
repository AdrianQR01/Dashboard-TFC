{
  "name": "dashboard-tfc",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "prisma generate && remix vite:build",
    "build:css": "tailwindcss -m -i ./tailwind.css -o app/tailwind.css",
    "dev": "remix vite:dev --host",
    "dev:css": "tailwindcss -w -i ./tailwind.css -o app/tailwind.css",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@prisma/client": "^5.15.0",
    "@prisma/extension-accelerate": "^1.1.0",
    "@remix-run/eslint-config": "^2.9.2",
    "@remix-run/node": "^2.9.1",
    "@remix-run/react": "^2.9.1",
    "@remix-run/serve": "^2.9.1",
    "@splinetool/react-spline": "^3.1.0",
    "@splinetool/runtime": "^1.6.0",
    "@vercel/remix": "^2.9.1",
    "apexcharts": "^3.49.0",
    "argon2": "^0.40.3",
    "cloudinary": "^2.2.0",
    "express": "^4.19.2",
    "flowbite": "^2.3.0",
    "flowbite-react": "^0.9.0",
    "isbot": "^4.1.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-form": "^1.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@remix-run/dev": "^2.9.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/formidable": "^3.4.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^15.3.0",
    "postcss": "^8.4.38",
    "prisma": "^5.15.0",
    "standard": "^17.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.1.6",
    "typescript-eslint": "^7.10.0",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
